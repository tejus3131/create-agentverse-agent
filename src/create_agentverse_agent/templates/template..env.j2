# Environment Configuration
ENV="{{env}}"  # Environment: production or development

# API Keys
AGENTVERSE_API_KEY="{{agentverse_api_key if agentverse_api_key else ''}}"  # Your Agentverse API Key

# Agent Configuration
AGENT_NAME="{{display_name}}"  # Name of the agent
AGENT_SEED_PHRASE="{{agent_seed_phrase}}"  # Seed phrase for agent initialization
AGENT_PORT="{{agent_port}}"  # Port on which the agent will run
AGENT_ROUTE="{{agent_route}}"  # Route for accessing the agent
AGENT_HANDLE="{{safe_name}}"  # Unique handle for the agent

# Hosting Configuration
HOSTING_ENDPOINT="{{hosting_endpoint}}"  # Endpoint for hosting the agent

# Message Processing Configuration
MAX_PROCESSED_MESSAGES="{{max_processed_messages}}"  # Maximum number of processed messages to track
PROCESSED_MESSAGE_TTL_MINUTES="{{processed_message_ttl_minutes}}"  # TTL for processed messages in minutes
CLEANUP_INTERVAL_SECONDS="{{cleanup_interval_seconds}}"  # Interval for cleanup tasks in seconds

# Rate limiting configuration
RATE_LIMIT_MAX_REQUESTS="{{rate_limit_max_requests}}"  # Maximum requests allowed
RATE_LIMIT_WINDOW_MINUTES="{{rate_limit_window_minutes}}"  # Rate limit window in minutes
